<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andriod.flashback_music_ui">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/team_12_black"
        android:label="Deja vu"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.andriod.deja_vu.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.andriod.deja_vu.activities.MainActivity" />
        <activity android:name="com.andriod.deja_vu.activities.PlaySongListActivity" />
        <activity android:name="com.andriod.deja_vu.activities.AlbumListActivity" />
        <activity android:name="com.andriod.deja_vu.activities.VibeModeTrackListActivity" />
        <activity android:name="com.andriod.deja_vu.activities.MediaPlayerActivity" />
        <activity android:name="com.andriod.deja_vu.activities.DownloadActivity" />
        <activity android:name="com.andriod.deja_vu.activities.GoogleSignInActivity"></activity>
    </application>

</manifest>